{
  "manifest_version" : 3,
  "name" : "Dedupe Tabs",
  "version" : "0.1",

  "description": "A plain text description",

  "content_scripts"  : [{
    "matches" : [
      "<all_urls>"
    ],
    "js" : ["content.js"]
  }],
  "background" : {
    "service_worker" : "background.js"
  },
  "permissions": [
    "tabs"
  ]
}